打包命令
mvn clean install -Dmaven.test.skip=true -Denv=dev
mvn clean install -Dmaven.test.skip=true -Denv=test
mvn clean install -Dmaven.test.skip=true -Denv=prod


mvn clean install deploy -Dmaven.test.skip=true -Denv=dev
mvn clean install deploy -Dmaven.test.skip=true -Denv=test
mvn clean install deploy -Dmaven.test.skip=true -Denv=prod