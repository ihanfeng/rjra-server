<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:util="http://www.springframework.org/schema/util"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
          http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
          http://www.springframework.org/schema/util
          http://www.springframework.org/schema/util/spring-util-3.2.xsd">
    <bean id="ageRowMapper" class="com.hdg.rjra.rdb.executer.age.rowmapper.AgeRowMapper"/>

    <bean id="saveExamineLog" class="com.hdg.rjra.rdb.executer.age.saveExamineLog">
        <property name="rowMapper" ref="ageRowMapper"></property>
    </bean>
    <bean id="findExamineLogByResourceId" class="com.hdg.rjra.rdb.executer.age.findExamineLogByResourceId">
        <property name="rowMapper" ref="ageRowMapper"></property>
    </bean>
    <util:map id="rdb-examine-log-executer">
        <entry key="saveExamineLog" value-ref="saveExamineLog"/>
        <entry key="findExamineLogByResourceId" value-ref="findExamineLogByResourceId"/>
    </util:map>
</beans>