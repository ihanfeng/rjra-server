<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:util="http://www.springframework.org/schema/util"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
          http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
          http://www.springframework.org/schema/util
          http://www.springframework.org/schema/util/spring-util-3.2.xsd">

    <bean id="employRowMapper" class="com.hdg.rjra.rdb.executer.employ.rowmapper.EmployRowMapper"/>

    <bean id="findEmployByEmployId" class="com.hdg.rjra.rdb.executer.employ.FindEmployByEmployId">
        <property name="rowMapper" ref="employRowMapper"></property>
    </bean>
    <bean id="findEmployByUserIdPager" class="com.hdg.rjra.rdb.executer.employ.FindEmployByUserIdPager">
        <property name="rowMapper" ref="employRowMapper"></property>
    </bean>
    <bean id="findEmployByCompanyIdPager" class="com.hdg.rjra.rdb.executer.employ.FindEmployByCompanyIdPager">
        <property name="rowMapper" ref="employRowMapper"></property>
    </bean>
    <bean id="saveEmploy" class="com.hdg.rjra.rdb.executer.employ.SaveEmploy">
        <property name="rowMapper" ref="employRowMapper"></property>
    </bean>
    <bean id="updateEmployResult" class="com.hdg.rjra.rdb.executer.employ.UpdateEmployResult">
        <property name="rowMapper" ref="employRowMapper"></property>
    </bean>
    <bean id="updateEmployEntryPlanTime" class="com.hdg.rjra.rdb.executer.employ.UpdateEmployEntryPlanTime">
        <property name="rowMapper" ref="employRowMapper"></property>
    </bean>
    <bean id="updateEmployReport" class="com.hdg.rjra.rdb.executer.employ.UpdateEmployReport">
        <property name="rowMapper" ref="employRowMapper"></property>
    </bean>
    <bean id="updateEmployCompanyStatus" class="com.hdg.rjra.rdb.executer.employ.UpdateEmployCompanyStatus">
        <property name="rowMapper" ref="employRowMapper"></property>
    </bean>
    <bean id="updateEmployUserStatus" class="com.hdg.rjra.rdb.executer.employ.UpdateEmployUserStatus">
        <property name="rowMapper" ref="employRowMapper"></property>
    </bean>
    <util:map id="rdb-employ-executer">
        <entry key="findEmployByEmployId" value-ref="findEmployByEmployId"/>
        <entry key="findEmployByUserIdPager" value-ref="findEmployByUserIdPager"/>
        <entry key="findEmployByCompanyIdPager" value-ref="findEmployByCompanyIdPager"/>
        <entry key="saveEmploy" value-ref="saveEmploy"/>
        <entry key="updateEmployResult" value-ref="updateEmployResult"/>
        <entry key="updateEmployEntryPlanTime" value-ref="updateEmployEntryPlanTime"/>
        <entry key="updateEmployReport" value-ref="updateEmployReport"/>
        <entry key="updateEmployCompanyStatus" value-ref="updateEmployCompanyStatus"/>
        <entry key="updateEmployUserStatus" value-ref="updateEmployUserStatus"/>
    </util:map>
</beans>